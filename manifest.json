{
    "manifest_version": 3,
    "name": "Asana Standup Extension",
    "version": "1.0",
    "description": "Standup extension for Asana",
    "icons": {
        "128": "images/icon-128.png"
    },
    "content_scripts": [
        {
            "js": [
                "lib/jquery.min.js",
                "scripts/main.js"
            ],
            "matches": [
                "https://app.asana.com/*"
            ],
            "run_at": "document_start"
        }
    ],
    "web_accessible_resources": [{
        "matches": ["<all_urls>"],
        "resources": ["*.svg"]
    }]
  }